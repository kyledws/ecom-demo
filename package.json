{
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "ava": "3.11.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-ava": "11.0.0",
    "eslint-plugin-react": "7.20.6",
    "prettier": "2.0.5",
    "typescript": "4.0.2"
  },
  "dependenciesMeta": {
    "serverless-offline": {
      "unplugged": true
    }
  },
  "name": "caching",
  "private": true,
  "scripts": {
    "build:watch": "tsc -b -w",
    "build:clean": "tsc -b --clean",
    "dev:data": "yarn workspace @package/data-service run dev",
    "dev:page": "yarn workspace @package/page-service run dev",
    "dev:tours": "yarn workspace @package/tours run dev",
    "lint": "eslint packages",
    "format": "prettier --write packages/*/src/**/*.{ts,tsx}",
    "host": "yarn workspace @package/tours run host",
    "pack:tours": "yarn workspace @package/tours run pack",
    "test": "ava"
  },
  "workspaces": [
    "packages/*"
  ]
}
